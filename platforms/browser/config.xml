<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.sleepnow.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Sleep Now</name>
    <description>
        Sleep Now
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Sleep Now
    </author>
    <preference name="776387162818475" />
